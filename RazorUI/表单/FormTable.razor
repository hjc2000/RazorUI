<div class="root"
	 style="width: @(Width);
			height:@(Height);">
	<div class="head"
		 style="padding: @(HeadPadding);
				border-bottom: @(HeadBorderBottom);">
		@Head
	</div>
	<div class="body_box"
		 style="padding: @(BodyPadding);">
		<div class="body"
			 style="grid-template-columns: @(BodyGridTemplateColumns);
					grid-column-gap: @(BodyColumnGap);
					grid-row-gap: @(BodyRowGap);
					align-content: @BodyAlignContent;
					align-items: @BodyAlignItems;
					justify-content: @BodyJustifyContent;
					justify-items: @BodyJustifyItems;
					overflow: @(BodyOverflow);">
			@Body
		</div>
	</div>
	<div class="foot"
		 style="padding: @FootPadding;">

		@Foot
	</div>
</div>
